{
  "name": "3d-llm-newfe",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "backend": ".\\scripts\\start-backend.bat",
    "backend:unix": "bash ./scripts/start-backend.sh",
    "backend:dev": ".\\scripts\\start-dev-backend.bat",
    "backend:dev:unix": "bash ./scripts/start-dev-backend.sh",
    "start:all": "npm run backend",
    "start:services": "npm run backend",
    "dev:all": "concurrently \"npm run backend:dev\" \"npm run dev\" --names \"backend,frontend\" --prefix-colors \"blue,green\" --kill-others-on-fail",
    "dev:all:unix": "concurrently \"npm run backend:dev:unix\" \"npm run dev\" --names \"backend,frontend\" --prefix-colors \"blue,green\" --kill-others-on-fail"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "marked": "^16.2.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.32",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/eslint-plugin": "^1.2.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "3.5.3",
    "tailwindcss": "^3.4.11",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  }
}
